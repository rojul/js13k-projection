{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "arrow-parens": false,
    "indent": [true, "spaces", 2],
    "interface-name": false,
    "max-classes-per-file": false,
    "member-access": false,
    "member-ordering": false,
    "no-bitwise": false,
    "no-console": false,
    "no-debugger": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "never"]
  }
}
